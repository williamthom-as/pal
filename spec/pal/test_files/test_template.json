{
  "metadata" : {
    "version" : "2022-04-02",
    "name" : "Test Template",
    "handler" : "AwsCur",
    "description" : "Testing template for Rspec tests"
  },
  "filters":  {
    "condition": "AND",
    "rules": [
      {
        "field": "lineItem/ProductCode",
        "type": "string",
        "operator": "equal",
        "value": "awskms"
      },{
        "field": "lineItem/BlendedCost",
        "type": "number",
        "operator": "greater",
        "value": 0
      }
    ]
  },
  "actions" : {
    "group_by" : ["lineItem/UsageAccountId", "lineItem/ResourceId"],
    "action" : {
      "type" : "sum",
      "property" : "lineItem/BlendedCost"
    }
  },
  "exporter" : {
    "types" : [{
      "name" : "table",
      "settings" : []
    }],
    "properties" : [
      "bill/BillingPeriodStartDate",
      "lineItem/ResourceId",
      "lineItem/UsageAccountId",
      "lineItem/Operation",
      "lineItem/LineItemDescription",
      "lineItem/BlendedCost"
    ]
  }
}

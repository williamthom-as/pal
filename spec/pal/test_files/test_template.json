{
  "metadata" : {
    "version" : "2022-04-02",
    "name" : "Test Template",
    "handler" : "AwsCur",
    "description" : "Testing template for Rspec tests"
  },
  "filters":  {
    "condition": "AND",
    "rules": [
      {
        "field": "lineItem/ProductCode",
        "type": "string",
        "operator": "equal",
        "value": "awskms"
      },{
        "field": "lineItem/BlendedCost",
        "type": "number",
        "operator": "greater",
        "value": 0
      },{
        "field": "lineItem/ResourceId",
        "type": "string",
        "operator": "is_not_empty",
        "value": ""
      }
    ]
  },
  "exporter" : {
    "types" : [{
      "name" : "table",
      "settings" : []
    }],
    "properties" : [
      "lineItem/UsageStartDate",
      "lineItem/ResourceId",
      "lineItem/UsageAccountId",
      "lineItem/Operation",
      "lineItem/LineItemDescription",
      "lineItem/BlendedCost"
    ],
    "actions" : {
      "group_by" : ["lineItem/UsageAccountId", "lineItem/ResourceId"],
      "projection" : {
        "type" : "sum",
        "property" : "lineItem/BlendedCost"
      }
    }
  }
}

{
  "metadata" : {
    "version" : "2022-04-02",
    "name" : "Test Template",
    "handler" : "AwsCur",
    "description" : "Testing template for Rspec tests"
  },
  "filters":  {
    "condition": "OR",
    "rules": [
      {
        "field": "lineItem/UsageType",
        "type": "string",
        "operator": "equal",
        "value": "APS2-AFS1-AWS-Out-Bytes"
      },
      {
        "condition": "AND",
        "rules": [
          {
            "field": "lineItem/UsageType",
            "type": "string",
            "operator": "equal",
            "value": "APS2-AFS2-AWS-Out-Bytes"
          }
        ]
      }
    ]
  },
  "export" : {
    "types" : [{
      "name" : "csv",
      "settings" : []
    }],
    "properties" : []
  }
}


{
  "metadata" : {
    "version" : "2022-04-02",
    "name" : "KMS Report",
    "handler" : "AwsCur",
    "description" : "List of billed KMS keys"
  },
  "filters":  {
    "condition": "AND",
    "rules": [{
        "field": "lineItem/BlendedCost",
        "type": "number",
        "operator": "greater",
        "value": 0
      },{
      "field": "lineItem/ProductCode",
      "type": "string",
      "operator": "equal",
      "value": "awskms"
    },{
      "field": "product/productFamily",
      "type": "string",
      "operator": "equal",
      "value": "Encryption Key"
    }]
  },
  "exporter" : {
    "types" : [{
      "name" : "csv",
      "settings" : {
        "file_name" : "pal_report_kms"
      }
    }, {
      "name" : "table",
      "settings" : {
        "title" : "AWS CUR Daily Combined Costs"
      }
    }, {
      "name" : "terminal",
      "settings" : {}
    }],
    "properties" : [
      "lineItem/UsageType",
      "lineItem/ProductCode",
      "product/productFamily",
      "lineItem/LineItemDescription",
      "lineItem/ResourceId",
      "lineItem/BlendedCost"
    ],
    "actions" : {
      "group_by" : ["product/productFamily", "lineItem/ResourceId"],
      "sort_by" : "lineItem/ResourceId",
      "projection" : {
        "type" : "sum",
        "property" : "lineItem/BlendedCost"
      }
    }
  }
}
